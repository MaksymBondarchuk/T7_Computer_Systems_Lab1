        <RadioButton x:Name="radioButton" Content="Method_1" HorizontalAlignment="Left" Margin="505,127,0,0" VerticalAlignment="Top" FontSize="18.667" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsChecked="True"/>
